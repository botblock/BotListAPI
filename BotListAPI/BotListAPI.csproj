<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Builderb</Authors>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Company>Fluxpoint Development</Company>
    <Version>5.4</Version>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Copyright>Copyright 2019 Fluxpoint Development</Copyright>
    <RepositoryUrl>https://github.com/botblock/BotListAPI</RepositoryUrl>
    <PackageProjectUrl>https://fluxpoint.dev</PackageProjectUrl>
    <PackageTags>discord</PackageTags>
    <Description>A C# lib for all of the Discord bot list APIs</Description>
    <PackageReleaseNotes>Added arcane botlist and added Bot to the start of discordbotlist.com token by default if it dosen't have it</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Net.WebSocket" Version="2.2.0-dev-20191226.1" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
